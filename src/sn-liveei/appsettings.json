{
  "sensenet": {
    "RepositoryUrl": "[sensenet repo url]",
    "Authentication": {
      "Authority": "[identity server url]",
      "ClientId": "[application id]",
      "ClientSecret": "[application secret token]",
      "Scope": "[application scope]"
    },
    "UploadFileExtensions": {
      "jpg": "Image",
      "jpeg": "Image",
      "gif": "Image",
      "png": "Image",
      "bmp": "Image",
      "svg": "Image",
      "svgz": "Image",
      "tif": "Image",
      "tiff": "Image",
      "DefaultContentType": "File"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}