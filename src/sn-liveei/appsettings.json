        "SavingState",
        "ExtensionData",
        "BrowseApplication",
        "Versions",
        "CheckInComments",
        "RejectReason",
        "Workspace",
        "BrowseUrl",
        "Sharing",
        "SharedWith",
        "SharedBy",
        "SharingMode",
        "SharingLevel",
        "Actions",
        "IsFile",
        "Children",
        "Publishable",
        "Locked",
        "Rate",
        "RateStr",
        "Tags",
        "Approvable",
        "IsFolder",
        "Icon",
        "WorkspaceSkin",
        "AvailableViews",
        "FieldSettingContents",
        "AvailableContentTypeFields",
        "OwnerWhenVisitor",
        "Image"
      ]
